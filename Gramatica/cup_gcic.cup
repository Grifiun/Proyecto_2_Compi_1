		{: 
			if(valor.getTipoValor().equals("string") == false){//no es string
				String msg =  "La funcion " + funcion.getLexema() + " solo acepta <string>, tipo actual <" + valor.getTipoValor() + ">";
				TokenError tokenErrorAux = new TokenError("ERROR SEMANTICO", funcion.getLexema(), msg, funcion.getLinea(), funcion.getColumna());
				tablaSimbolos.addErrorSemantico(tokenErrorAux);
			}

			RESULT = valor;
			
		:}
	| FUNCTION_CARACTER_ALEATORIO PAREN_INI PAREN_FIN 
		{: 
			System.out.println ("Se ejecuto CHAR ALEATORIO "); 
			ValorElemento valorElementoAux = new ValorElemento("0", "char");
			RESULT = valorElementoAux;
		:}
	| FUNCTION_NUM_ALEATORIO PAREN_INI PAREN_FIN 
		{: 
			System.out.println ("Se ejecuto NUM ALEATORIO ");
			ValorElemento valorElementoAux = new ValorElemento("'x'", "integer");
			RESULT = valorElementoAux;
		:}
	//| error
	;
funciones_con_valor_tipo ::= FUNCTION_ASC:a {: RESULT = a; System.out.println ("Se ejecuto FUN ASC "); variablesAux += " ASC"; :}
	| FUNCTION_DESC:a {: RESULT = a; System.out.println ("Se ejecuto FUN DESC "); variablesAux += " DESC"; :}
	| FUNCTION_LETPAR_NUM:a {: RESULT = a; System.out.println ("Se ejecuto FUN LET NUM PAR "); variablesAux += " LETPAR_NUM"; :}
	| FUNCTION_LETIMPAR_NUM:a {: RESULT = a; System.out.println ("Se ejecuto FUN LET NUM IMPAR "); variablesAux += " LETIMPAR_NUM"; :}
	| FUNCTION_REVERSE:a	 {: RESULT = a; System.out.println ("Se ejecuto FUN REVERSE "); variablesAux += " REVERSE"; :}
	//| error
	;
funciones_con_valor_parametros ::= PAREN_INI {: variablesAux += "("; :} operacion_aritmetica_con_parentesis:valor PAREN_FIN {: RESULT = valor; variablesAux += ")"; :}
	| error
	;

funciones_instruccion ::= FUNCTION_ALERT_INFO {: variablesAux = ""; :} funciones_con_valor_parametros {: System.out.println ("Se ejecuto FUN ALERT "); contenidoScript += tab + "alert" + variablesAux + ";\n"; :}

	public void agregarTextoInsert(String texto){
		texto = removerCS.removerComillasSimples(texto);

		if(isRepeat == true){//estamos en el ciclo repeat
			System.out.println("Agregamos repeat insert: " + texto);
			ValorElemento aux = new ValorElemento(texto, tipoInsert);			
			elementosRepeat.add(aux);
		}else{//no está en un ciclo
			textoInsert += " " + texto + "\n";
		}
	}


	public void analizarTextoInsert(){
		//Ejecutamos el parser
        //texto = texto.substring(1, texto.length() - 1).trim();
		System.out.println("INTENTO ANALIZAR");

		String texto = textoInsert.trim();       
		if(texto.equals("") == true){
			return;//terminamos la funcion porque no hay texto que analizar
		}else{
			System.out.println("TEXTO INSERT A ANALIZAR");
			System.out.println(texto);
		}

        try{                    
			//Palabra descorrompida
			
			StringReader sr = new StringReader(texto);
			LexerGCICInsert lexer = new LexerGCICInsert(sr);
			ParserInsert pars = new ParserInsert(lexer, listadoErroresSintacticos, tablaSimbolos, tablaSimbolosEtiquetas, filaInsert, columnaInsert, bloqueHTML, numeroEtiqueta + 1, listadoEtiquetaActual());

			try{                        
				pars.parse();   
			}catch(Exception ex){
				System.out.println("Error en el lenguajes de etqiuetas insert: "+ex.getMessage());
				ex.printStackTrace();
			}finally{
				numeroEtiqueta = pars.getNumeroEtiqueta();
			}

			System.out.println(" Parser insert Ejecutado");
                
		}catch(Exception ex){
			System.out.println("Error al ejecutar el parser insert: "+ex.getLocalizedMessage());
		}

		textoInsert = ""; //reiniciamos el texto insert
	}

